{
  "presets": [
    "es2015",
    "stage-2",
    "react"
  ],
  "plugins": [
    "add-module-exports",
    "transform-runtime",
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel",
        ["transform-runtime", {
          "polyfill": true,
          "regenerator": true
        }]
      ]
    }
  }
}
