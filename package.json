  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production npm-run-all --parallel production-build:client production-build:server",
    "bundle": "NODE_ENV=production webpack --config ./webpack.client.config.js --bail",
    "development-build:client": "node ./development/webpack-dev-server.js",
    "development-build:server": "webpack --watch --config ./webpack.server.config.js",
    "development-start-server": "nodemon --trace-warnings development/run-src-server.js -w dist -w development/run-src-server.js",
    "postbuild": "mv dist/public/webpack-chunks.json dist; cp development/server.production.js dist/server.js",
    "prebuild": "universal-webpack --settings ./development/universal-webpack-settings.js prepare",
    "preserve": "npm run build",
    "prestart": "npm run prebuild",
    "production-build:client": "webpack --config ./webpack.client.config.js --bail",
    "production-build:server": "webpack --config ./webpack.server.config.js --bail",
    "production-start-server": "node ./development/run-src-server.js",
    "serve": "NODE_ENV=development node development/run-dist-server",
    "start": "NODE_ENV=development npm-run-all --parallel development-build:client development-build:server development-start-server"
  },
